{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
      "include": "./src/",
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,
      "default": {
        "staticFiles": {
          "paths": [
            "./src/static"
          ]
        }
      }
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": true,
    "tutorials": "./tutorials"
  }
}